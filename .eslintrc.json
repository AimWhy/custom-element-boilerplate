{
  "extends": [
    "plugin:github/browser",
    "plugin:github/es6"
  ],
  "globals": {
    "CustomElementElement": "readonly"
  },
  "overrides": [
    {
      "files": "test/*.js",
      "rules": {
        "github/unescaped-html-literal": "off"
      }
    },
    {
      "files": "*.ts",
      "extends": [
        "plugin:github/typescript"
      ]
    }
  ]
}
